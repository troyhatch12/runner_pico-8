pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

player = {}
player.x = 44
player.y = 44
player.speed = 1
player.sprite = 1
player.di = 0
player.moving = false


game_state = "menu"

counter = 0
timer = 0

music(1)


function add_actor()

end

--animations
--
--
function move_anim_player()
	player.moving = true
	
	if (player.di == 0) then
		player.sprite = seq_anim(player.sprite,4,5,5)
	elseif (player.di == 1) then
		player.sprite = seq_anim(player.sprite,2,3,5)
	elseif (player.di == 2) then
		player.sprite = seq_anim(player.sprite,6,7,5)	
	elseif (player.di == 3) then 
		player.sprite = seq_anim(player.sprite,8,9,5)
	end	
end

function draw_still()
	if (player.di == 0) then
		player.sprite = 1
	elseif (player.di == 1) then
		player.sprite = 17
	elseif (player.di == 2) then
		player.sprite = 10
	elseif (player.di == 3) then
		player.sprite = 11
	end
end

function seq_anim(s,first,last,a_rate)
	-- sequentially animate the sprite(s)
	-- from the first frame of the animation
	-- to the last frame of the animation.
	-- a_rate is the number of game frames 
	-- between each sprite change.
	if ((s < first) or (s > last))	then
		s = first
		end
		
	if ((counter % a_rate) == 0) s+=1
	
	if (s > last) then
		s = first
		end
	return s
end
--
--

--updates
--
--
function _update()

	if (game_state == "menu") then
		menu_update()
	elseif (game_state == "game") then
		game_update()
	end
		
end

function menu_update()
	if (btnp(5)) game_state = "game"
end

function game_update()
	
	player.moving = false

--listen for button presses	
	if (btn(0))	then
		player.x-=player.speed	
		player.di = 0		
		move_anim_player()
	end
	
	if (btn(1)) then
		player.x+=player.speed 
		player.di = 1
		move_anim_player()
	end
	
	if (btn(2)) then
		player.y-=player.speed
		player.di = 2
		move_anim_player()		
	end
	
	if (btn(3)) then
	 player.y+=player.speed 
		player.di = 3
		move_anim_player()
	end
	
	if (btnp(5)) then
		bullet_shoot()
	end
		
	
	if (player.moving == false) then
		draw_still()
		end
		
	counter+=1
	
	if ((counter % 30) == 0) timer+=1
	
end
--
--

--draws
--
--
function _draw()
	if (game_state == "menu") then
		menu_draw()
	elseif (game_state == "game") then
		game_draw()
	end
end

function menu_draw()
cls()
	print("press 'x' to play game",20,40)
end

function game_draw()
	//clear screen every frame
	cls()
	print(timer,20,20)	
	//draw little dude
	spr(player.sprite,player.x,player.y)
	
end
